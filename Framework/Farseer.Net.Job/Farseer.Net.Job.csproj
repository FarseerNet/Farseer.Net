<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Company>steden</Company>
        <Copyright>http://www.fsgit.com</Copyright>
        <Authors>steden</Authors>
        <Product>Job组件</Product>
        <Description>包括：Job执行（定时、手动）</Description>
        <AssemblyVersion>2.5.0</AssemblyVersion>
        <FileVersion>2.5.0</FileVersion>
        <Version>2.5.0</Version>
        <PackageTags>Job组件</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <RootNamespace>FS.Job</RootNamespace>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\Farseer.Net.Grpc\Farseer.Net.Grpc.csproj" />
        <ProjectReference Include="..\Farseer.Net.Utils\Farseer.Net.Utils.csproj" />
        <ProjectReference Include="..\Farseer.Net\Farseer.Net.csproj" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Grpc" Version="2.37.0" />
        <PackageReference Include="Grpc.Core" Version="2.37.0" />
        <PackageReference Include="Grpc.Tools" Version="2.37.0">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Google.Protobuf" Version="3.15.8" />
        <PackageReference Include="Grpc.Net.Client" Version="2.37.0" />
        <PackageReference Include="Microsoft.Win32.Registry" Version="5.0.0" />
        <PackageReference Include="Snowflake.Core" Version="2.0.0" />
        <PackageReference Include="System.ServiceProcess.ServiceController" Version="5.0.0" />
    </ItemGroup>

    <ItemGroup>
        <Protobuf Include="Protos\Server\receiveNotify.proto" GrpcServices="Server" />
        <Protobuf Include="Protos\Client\registerCenter.proto" GrpcServices="Client" />
        <Protobuf Include="Protos\Client\rpcResponse.proto" GrpcServices="Client" />
    </ItemGroup>
</Project>